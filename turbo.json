{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NEXT_PUBLIC_*",
    "GITHUB_TOKEN",
    "I18NEXUS_API_KEY",
    "I18NEXUS_VERSION",
    "I18NEXUS_API_URL",
    "I18NEXUS_PRE_BUILD",
    "VERCEL_ENV",
    "NODE_ENV",
    "MONGODB_URI"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test": {
      "outputs": ["coverage/"],
      "dependsOn": []
    },
    "lint": {
      "outputs": []
    },
    "lint:faucet": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "dev:web": {
      "cache": false
    },
    "dev:bridge": {
      "env": [
        "NEXT_PUBLIC_INFURA_API_KEY",
        "NEXT_PUBLIC_SITE_URL",
        "NEXT_PUBLIC_VERCEL_URL",
        "NODE_ENV",
        "MONGODB_URI",
        "NEXT_PUBLIC_L1_CHAIN_ID",
        "NEXT_PUBLIC_L2_CHAIN_ID",
        "SUPAGRAPH_NAME",
        "SUPAGRAPH_L1_START_BLOCK",
        "SUPAGRAPH_L1_STATE_COMMITMENT_CHAIN"
      ],
      "cache": false
    },
    "dev:faucet": {
      "env": [
        "TWITTER_ID",
        "TWITTER_SECRET",
        "NEXTAUTH_URL",
        "NEXTAUTH_SECRET"
      ],
      "cache": false
    },
    "dev:converter": {
      "env": [
        "NEXT_PUBLIC_INFURA_API_KEY",
        "NEXT_PUBLIC_SITE_URL",
        "NEXT_PUBLIC_VERCEL_URL",
        "NEXT_PUBLIC_L1_CHAIN_ID",
        "NEXT_PUBLIC_L2_CHAIN_ID",
        "NEXT_PUBLIC_IS_COMING_SOON"
      ],
      "cache": false
    },
    "dev:charity": {
      "env": [
        "NEXT_PUBLIC_INFURA_API_KEY",
        "NEXT_PUBLIC_SITE_URL",
        "NEXT_PUBLIC_VERCEL_URL",
        "NEXT_PUBLIC_L1_CHAIN_ID",
        "NEXT_PUBLIC_L2_CHAIN_ID",
        "NEXT_PUBLIC_IS_COMING_SOON"
      ],
      "cache": false
    },
    "dev:token": {
      "cache": false,
      "env": [
        "SUPAGRAPH_NAME",
        "MANTLE_CHAIN_ID",
        "MANTLE_ADDRESS",
        "MANTLE_START_BLOCK",
        "MANTLE_END_BLOCK",
        "BITDAO_CHAIN_ID",
        "BITDAO_ADDRESS",
        "BITDAO_START_BLOCK",
        "BITDAO_END_BLOCK"
      ]
    },
    "dev:cms": {
      "cache": false
    },
    "dev:events": {
      "cache": false
    },
    "dev:template": {
      "cache": false
    },
    "e2e:test:template": {
      "cache": false
    }
  }
}
