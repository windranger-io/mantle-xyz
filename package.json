{
  "name": "mantle-xyz",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "ci:template": "turbo e2e:test:template",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev:web",
    "dev:faucet": "turbo run dev:faucet",
    "dev:bridge": "turbo run dev:bridge",
    "dev:cms": "turbo run dev:cms",
    "dev:template": "turbo run dev:template",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@mantle/eslint-config-next": "workspace:*",
    "@types/node": "18.11.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "packageManager": "pnpm@7.18.2"
}
